# Include Go shared libraries
include orion/backend/lattigo/*.so
include orion/backend/lattigo/*.dll
include orion/backend/lattigo/*.dylib

# Include other necessary files
include LICENSE
include README.md

# Exclude common Python cache files
global-exclude __pycache__
global-exclude *.py[cod]
global-exclude *.so.dSYM
global-exclude .git*
global-exclude .DS_Store